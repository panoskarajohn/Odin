{
  "app": {
    "name": "Slip.Api",
    "service": "slip-service",
    "version": "1.0.0"
  },
  "redis": {
    "ConnectionString": "localhost:6379"
  },
  "logger": {
    "applicationName": "slip-service",
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "prometheus": {
    "enabled": true,
    "endpoint": "/metrics"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5,
    "tags": {
      "app": "slip-service"
    }
  },
  "swagger": {
    "enabled": true,
    "title": "Slip API",
    "version": "v1",
    "route": "swagger"
  },
  "auth": {
    "certificate": {
      "location": "certs/localhost.pfx",
      "password": "test",
      "rawData": ""
    },
    "jwt": {
      "issuer": "odin",
      "audience": "odin",
      "validIssuer": "odin",
      "validAudience": "odin",
      "validateAudience": true,
      "validateIssuer": true,
      "validateLifetime": true
    }
  },
  "rabbitMq": {
    "connectionName": "slip-service",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "slip"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "slip-service/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context"
  },
  "urls": {
    "EventGrpcUrl": "http://localhost:4081"
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999"
  },
  "httpClient": {
    "name": "",
    "type": "",
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    },
    "services": {}
  },
  "consul": {
    "enabled": true,
    "isGrpc" : false,
    "url": "http://localhost:8500",
    "service": {
      "name": "event-service",
      "url": "http://host.docker.internal:2000"
    },
    "healthCheck": {
      "endpoint": "ping",
      "interval": "00.00:00:05",
      "deregisterInterval": "00.00:00:05"
    }
  },
  "AllowedHosts": "*"
}
