{
  "logger": {
    "applicationName": "identity-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": true,
      "url": "http://seq:5341",
      "token": "secret"
    }
  },
  "postgres": {
    "connectionString": "Host=postgres,5432;Database=odin-users;Username=postgres;Password=postgres"
  }
}