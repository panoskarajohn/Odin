{
  "app": {
    "name": "Event.Api",
    "version": "1.0.0"
  },
  "logger": {
    "applicationName": "event-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "event-service",
    "seed": false
  },
  "AllowedHosts": "*"
}