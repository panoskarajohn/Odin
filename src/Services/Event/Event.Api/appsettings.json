{
  "app": {
    "name": "Event.Api",
    "service": "event-service",
    "version": "1.0.0"
  },
  "logger": {
    "applicationName": "event-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "mongo": {
    "connectionString": "mongodb://root:rootpassword@localhost:27017",
    "database": "event-service",
    "seed": false
  },
  "prometheus": {
    "enabled": true,
    "endpoint": "/metrics"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5,
    "tags": {
      "app": "event-service"
    }
  },
  "auth": {
    "certificate": {
      "location": "certs/localhost.pfx",
      "password": "test",
      "rawData": ""
    },
    "jwt": {
      "issuer": "odin",
      "audience": "odin",
      "validIssuer": "odin",
      "validAudience": "odin",
      "validateAudience": true,
      "validateIssuer": true,
      "validateLifetime": true
    }
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999"
  },
  "httpClient": {
    "name": "",
    "type": "",
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    },
    "services": {}
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": {
      "name": "event-service",
      "url": "http://host.docker.internal:2000"
    },
    "healthCheck": {
      "endpoint": "ping",
      "interval": "00.00:00:05",
      "deregisterInterval": "00.00:00:05"
    }
  },
  "AllowedHosts": "*"
}