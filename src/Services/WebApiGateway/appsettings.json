{
  "app": {
    "name": "Odin.Gateway",
    "service": "gateway-service",
    "version": "1.0.0"
  },
  "auth": {
    "certificate": {
      "location": "certs/localhost.pfx",
      "password": "test",
      "rawData": ""
    },
    "jwt": {
      "issuer": "odin",
      "audience": "odin",
      "validIssuer": "odin",
      "validAudience": "odin",
      "validateAudience": true,
      "validateIssuer": true,
      "validateLifetime": true
    }
  },
  "logger": {
    "applicationName": "api-gateway",
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999"
  },
  "httpClient": {
    "name": "",
    "type": "",
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    },
    "services": {}
  },
  "urls": {
    "eventService": "http://localhost:9999/event-service",
    "identityService": "http://localhost:9999",
    "slipService": "http://localhost:9999"
  },
  "consul": {
    "enabled": false,
    "isGrpc": false,
    "url": "http://localhost:8500",
    "service": {
      "name": "gateway-service",
      "url": "http://host.docker.internal:1000"
    },
    "healthCheck": {
      "endpoint": "ping",
      "interval": "00.00:00:05",
      "deregisterInterval": "00.00:00:05"
    }
  },
  "AllowedHosts": "*"
}
