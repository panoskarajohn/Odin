{
  "logger": {
    "applicationName": "api-gateway",
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": true,
      "url": "http://seq:5341",
      "token": "secret"
    }
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999"
  },
  "httpClient": {
    "name": "",
    "type": "",
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    },
    "services": {}
  },
  "urls": {
    "eventService": "http://fabio:9999/event-service",
    "identityService": "http://localhost:9999",
    "slipService": "http://localhost:9999"
  },
  "consul": {
    "enabled": false,
    "isGrpc": false,
    "url": "http://consul",
    "service": {
      "name": "gateway-service",
      "url": "http://api.gateway"
    },
    "healthCheck": {
      "endpoint": "ping",
      "interval": "00.00:00:05",
      "deregisterInterval": "00.00:00:05"
    }
  },
  "AllowedHosts": "*"
}
